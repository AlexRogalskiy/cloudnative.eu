{
  "hosting": {
    "public": "_site",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/feed",
        "destination": "https://cloudnative.eu/feed.xml",
        "type": 301
      },
      {
        "source": "/slack*",
        "destination": "https://workshops.de/slack",
        "type": 301
      },
      {
        "source": "/chat*",
        "destination": "https://cloudnative.eu/slack",
        "type": 301
      },
      {
        "source": "/schulungen/angular-advanced/",
        "destination": "https://cloudnative.eu/schulungen/angular-enterprise-applications/",
        "type": 301
      },
      {
        "source": "/workshops/",
        "destination": "https://cloudnative.eu/schulungen/",
        "type": 301
      },
      {
        "source": "/workshops/*",
        "destination": "https://cloudnative.eu/schulungen/",
        "type": 301
      },
      {
        "source": "/inhouse-workshop-anfrage",
        "destination": "https://cloudnative.eu/inhouse-workshop/",
        "type": 301
      },
      {
        "source": "/veranstaltungen*",
        "destination": "https://cloudnative.eu/schulungen/",
        "type": 301
      },
      {
        "source": "/entwickler*",
        "destination": "https://cloudnative.eu/",
        "type": 301
      },
      {
        "source": "/consulting*",
        "destination": "https://cloudnative.eu/",
        "type": 301
      },
      {
        "source": "/projekte*",
        "destination": "https://cloudnative.eu/",
        "type": 301
      },
      {
        "source": "/classroom/:room_id*",
        "destination": "https://workshops.de/classroom/:room_id*",
        "type": 301
      },
      {
        "source": "/buch/:id*",
        "destination": "https://cloudnative.eu/buecher/angularjs-buch/:id*",
        "type": 301
      },
      {
        "source": "/blog/:id*",
        "destination": "https://cloudnative.eu/artikel/:id",
        "type": 301
      },
      {
        "source": "/artikel/angular2-bootstrap-scss-angular-cli*",
        "destination": "https://cloudnative.eu/artikel/angular-bootstrap-scss-angular-cli/",
        "type": 301
      },
      {
        "source": "/artikel/angular2-tutorial-deutsch*",
        "destination": "https://cloudnative.eu/artikel/angular-tutorial-deutsch/",
        "type": 301
      },
      {
        "source": "/artikel/ngupgrade-migration-angularjs-angular2*",
        "destination": "https://cloudnative.eu/artikel/ngupgrade-migration-angularjs-angular/",
        "type": 301
      },
      {
        "source": "/artikel/angular2-routing-tutorial-deutsch*",
        "destination": "https://cloudnative.eu/artikel/angular-routing-tutorial-deutsch/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=604800"
          }
        ]
      },
      {
        "source": "/assets/mirror/**",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "noindex, follow"
          }
        ]
      }
    ]
  }
}